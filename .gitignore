# Ignore bundler config
/.bundle

# Ignore the default SQLite database
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles
/log/*.log
/tmp

# Ignore local Guardfile customation file
config/local_guardfile_customizations.yml

# Ignore editor files
*~
\#*\#

# Ignore config files with sensitive data
database.yml
fedora.yml
hyacinth.yml
secrets.yml
solr.yml
repository_cache.yml

*_user_accounts.yml

# subs.yml doesn't need to be stored here because it's not currently related to Hyacinth
config/subs.yml

# Ignore hydra-jetty
/jetty
/jetty-test

# Ignore OS-specific
.DS_Store

# Ignore application project files
*.komodoproject

# Ignore elo2112-specific
reference-files/*

